{
  "name": "ducky",
  "version": "0.0.0",
  "description": "A general-purpose Discord bot",
  "main": "out/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "compile": "npm run lint && npm run format && rm -rf ./out && npm run build",
    "dev": "npm run compile && node ./out/index.js"
  },
  "dependencies": {
    "@n-f9/quack.js": "^1.0.0",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.14",
    "@types/validator": "^13.7.1",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.4"
  }
}